
.multiTitle{
	width: 100%;
}
.multiTitle image {
	width: 100%;
	height: 116rpx;
}

.text {
	padding: 60rpx;
	font-size: 28rpx;
	letter-spacing: 2rpx;
	color: #666;
}

.slider {
	padding: 30rpx;
}

.navtitle {
	padding-left: 30rpx;
	font-size: 32rpx;
	color: #de0000;
}

.groupimg {
	padding: 10rpx 30rpx;
	padding-bottom: 40rpx;
	border-bottom: 1rpx solid #f4f4f4;
}
.groupimg .imgtitle {
	font-size: 36rpx;
	line-height: 50rpx;
}
.groupimg .imgs {
	width: 100%;
	display: flex;
	flex-direction: row;
	justify-content: space-between;
	-webkit-justify-content: space-between;
}
.groupimg .imgs .img {
	flex: 0 0 226rpx;
	height: 150rpx;
}
